<h1> Search by Quantity </h1>

<%= form_with url: inventories_path, method: :get do |p| %>

  <h3><%= p.label "Number Input " %><%= p.text_field :quantity %></h3>
  <h3><%= p.submit "Search" %></h3>
<% end %><br/><br/>

<%= render(partial: 'inventories', locals: {inventories: @inventories, index: true}) %>