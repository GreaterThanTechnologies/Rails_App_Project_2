<h1>Log In</h1>

<%=render(partial: "application/errors", locals: {errors: @errors})%>


<%= form_for :owner do |f|%>
  <h3><%= f.label "Username " %><%= f.text_field :username %></h3>
  <h3><%= f.label "Password " %><%= f.password_field :password %></h3>
  <h3><%= f.submit "Get In" %></h3>
<% end %>